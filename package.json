  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-melon/melon-core.git"
  },
  "peerDependencies": {
    "react": "^15.6.2||^16.0.0",
    "react-dom": "^15.6.2||^16.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "components"
  ],
