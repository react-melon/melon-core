{
  "name": "melon-core",
  "version": "0.3.1",
  "description": "React ui components base project",
  "main": "./src",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "example"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "expect": "^1.20.1",
    "expect-jsx": "^2.6.0",
    "isparta": "^4.0.0",
    "jsdom": "^8.5.0",
    "mime": "^1.3.4",
    "mocha": "^2.5.3",
    "moment": "^2.13.0",
    "nib": "^1.1.0",
    "react-addons-test-utils": "^0.14.8",
    "stylus": "^0.54.5",
    "swig": "^1.4.2"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha test/components/** --compilers js:babel-core/register --require ./test/setup.js",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover --report html ./node_modules/.bin/_mocha -- test/components/** --require ./test/setup.js",
    "lint": "fecs src test --rule",
    "build": "sh tool/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinzhubaofu/melon.git"
  },
  "peerDependencies": {
    "react": ">=0.14"
  },
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "author": "ludafa@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/react-melon/melon-core/issues"
  },
  "homepage": "https://github.com/react-melon/melon-core#readme",
  "edp": {
    "wwwroot": "/",
    "depDir": "dep",
    "srcDir": "src",
    "loaderAutoConfig": "js,htm,html,tpl,vm,phtml",
    "loaderUrl": "http://s1.bdstatic.com/r/www/cache/ecom/esl/1-8-2/esl.js",
    "dependencies": {}
  },
  "dependencies": {
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  }
